{
    "name": "ilias-pegasus",
    "private": true,
    "version": "4.5.0",
    "scripts": {
        "start": "ionic cordova emulate",
        "build:release:ios": "NODE_OPTIONS=--max_old_space_size=4096 ionic cordova build ios --release --prod --device",
        "build:release:android": "NODE_OPTIONS=--max_old_space_size=4096 ionic cordova build android --release --prod",
        "lint": "ng lint app",
        "pretest:ci": "node ./tools/hooks/preTest.js",
        "test": "npm run ionic:build:before && ng test",
        "test:ci": "npm run ionic:build:before && ng test --no-watch --no-progress --browsers=ChromeHeadlessCI",
        "test:safari": "npm run ionic:build:before && ng test --browsers Safari",
        "setbrand": "node ./branding/set_brand.js",
        "ionic:build:before": "node ./tools/hooks/ionicBuildBefore.js",
        "ionic:serve:before": "node ./tools/hooks/ionicServeBefore.js"
    },
